{
  "name": "justforview-store",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:socket\" --names \"NEXT,SOCKET\" --prefix-colors \"cyan,magenta\"",
    "dev:next": "next dev --turbopack",
    "dev:next-webpack": "next dev --webpack",
    "dev:socket": "node server.js",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "node server.js",
    "start:next": "next start",
    "start:socket": "node server.js",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "perf:lighthouse": "lighthouse http://localhost:3000 --view --output=html --output-path=./lighthouse-report.html",
    "perf:bundle": "npm run build:analyze",
    "firebase:init": "node scripts/init-firebase.js",
    "firebase:init-data": "node scripts/init-firebase.js --init-data",
    "firebase:create-admin": "node scripts/init-firebase.js --create-admin",
    "firebase:env-setup": "node scripts/init-firebase.js --env-setup",
    "firebase:deploy": "firebase deploy",
    "firebase:deploy-rules": "firebase deploy --only firestore:rules,storage",
    "firebase:deploy-hosting": "npm run build && firebase deploy --only hosting",
    "firebase:emulator": "firebase emulators:start",
    "firebase:emulator-ui": "firebase emulators:start --import=./firebase-emulator-data --export-on-exit=./firebase-emulator-data",
    "build:static": "node scripts/build-static.js",
    "deploy": "pwsh -File deploy-with-seo.ps1",
    "deploy:preview": "pwsh -File deploy-with-seo.ps1 -Preview",
    "deploy:production": "pwsh -File deploy-with-seo.ps1 -Production",
    "deploy:vercel": "vercel --prod",
    "deploy:check": "pwsh -File pre-deploy-checklist.ps1",
    "deploy:setup": "pwsh -File deploy-to-vercel.ps1",
    "deploy:seo": "pwsh -File deploy-with-seo.ps1",
    "vercel:env-sync": "pwsh -File sync-env-to-vercel.ps1",
    "vercel:setup": "npm run vercel:env-sync && vercel --prod",
    "beyblade:init": "node scripts/init-beyblade-stats.js"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.8",
    "@fontsource/roboto": "^5.2.8",
    "@hello-pangea/dnd": "^18.0.1",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@types/js-cookie": "^3.0.6",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie": "^0.6.0",
    "critters": "^0.0.25",
    "crypto": "^1.0.1",
    "date-fns": "^3.0.0",
    "firebase": "^12.4.0",
    "firebase-admin": "^12.0.0",
    "framer-motion": "^11.0.0",
    "gray-matter": "^4.0.3",
    "ioredis": "^5.8.2",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.547.0",
    "moment": "^2.29.4",
    "next": "^16.0.0",
    "next-themes": "^0.2.1",
    "node-cache": "^5.1.2",
    "papaparse": "^5.5.3",
    "pixi.js": "^8.14.0",
    "razorpay": "^2.9.6",
    "react": "^18.3.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.0",
    "react-easy-crop": "^5.5.3",
    "react-hook-form": "^7.65.0",
    "react-hot-toast": "^2.4.1",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "swr": "^2.2.4",
    "tailwind-merge": "^3.3.1",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie": "^0.6.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.0",
    "@types/papaparse": "^5.3.16",
    "@types/paypal__checkout-server-sdk": "^1.0.8",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "autoprefixer": "^10.4.17",
    "concurrently": "^9.2.1",
    "eslint": "^9.0.0",
    "eslint-config-next": "^16.0.0",
    "jest": "^30.2.0",
    "postcss": "^8.4.33",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.5",
    "typescript": "^5.3.0"
  }
}
